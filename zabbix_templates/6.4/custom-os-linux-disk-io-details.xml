            <discovery_rules>
                <discovery_rule>
                    <uuid>6e50927ae08242c0a136e53e23500428</uuid>
                    <name>Disk IO detail discovery</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.dev.discovery.detail</key>
                    <delay>60m</delay>
                    <filter>
                        <conditions>
